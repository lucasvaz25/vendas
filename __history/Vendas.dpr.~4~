  Application.MainFormOnTaskbar := True;
  Application.CreateForm(TSistema, Sistema);
  Application.CreateForm(TFRM_PDV, FRM_PDV);
  //  Application.CreateForm(TFRM_CAD_BASE, FRM_CAD_BASE);
  Application.Run;
end.
