program Vendas;

uses
  Vcl.Forms,
  USistema in 'USistema.pas' {Sistema},
  UFRM_CAD_BASE in 'UFRM_CAD_BASE.pas' {FRM_CAD_BASE},
  UFRM_PDV in 'UFRM_PDV.pas' {FRM_PDV};

{$R *.res}

begin
